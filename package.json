{
  "name": "kiko",
  "version": "1.0.0",
  "description": "Recruitment web app",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --exec ts-node src/server/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node dist/server/server.js",
    "grunt": "grunt",
    "build": "grunt build",
    "pretest": "tslint",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm migration:create src/app/db/entity/migrations/${name}",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/app/db/entity/kiko/index.ts",
    "migration:run-prod": "node ./node_modules/typeorm/cli.js migration:run -d dist/app/db/entity/kiko/index.js",
    "migration:rollback": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/app/db/entity/kiko/index.ts",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js -d src/app/db/entity/kiko/index.ts seed src/app/db/entity/seeds/*.ts",
    "seed:run-single": "ts-node -r tsconfig-paths/register ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js -d src/app/db/entity/kiko/index.ts seed src/app/db/entity/seeds/${npm_config_name}.seeder.ts"
  },
  "author": "Hemant Vinchur",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.18.33",
    "grunt": "^1.0.4",
    "grunt-concurrent": "^3.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-shell": "^3.0.1",
    "grunt-ts": "^6.0.0-beta.22",
    "merge2": "^1.2.3",
    "require-dir": "^1.2.0",
    "ts-loader": "^8.0.4",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@jorgebodega/typeorm-seeding": "^6.0.0",
    "@koa/cors": "^4.0.0",
    "@sap/hana-client": "^2.14.22",
    "@supercharge/request-ip": "^1.2.0",
    "@types/glob": "^7.1.1",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-convert": "^1.2.2",
    "@types/koa-generic-session": "^1.0.2",
    "@types/koa-helmet": "^5.2.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-static": "^4.0.1",
    "aws-sdk": "^2.1267.0",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "eslint": "^8.27.0",
    "exceljs": "^4.3.0",
    "grunt-yaml": "^0.4.2",
    "handlebars": "^4.7.7",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "just-handlebars-helpers": "^1.0.19",
    "koa": "^2.7.0",
